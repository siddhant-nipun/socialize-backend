{
    "defaultEnv": "pg",
    "pg": {
        "driver": "pg",
        "user": {
            "ENV": "PGUSER"
        },
        "host": {
            "ENV": "PGHOST"
        },
        "database": {
            "ENV": "PGDATABASE"
        },
        "password": {
            "ENV": "PGPASSWORD"
        },
        "port": {
            "ENV": "PGPORT"
        }
    }
}